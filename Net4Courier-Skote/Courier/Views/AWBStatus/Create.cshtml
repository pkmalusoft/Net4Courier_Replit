                        else {
                            Swal.fire("Data Validation!", response.message, "error");
                            $('#AWBNo').val('');
                            $('#AWBNo').focus();
                        }
                    }
                });
            }
            else if ($('#InScanID').val() > 0) {
                $('#lierr').html('');
