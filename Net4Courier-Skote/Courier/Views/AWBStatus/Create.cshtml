                var Shipper = $('#Consignor').val();
                var Receiver = $('#Consignee').val();
                var ConsignorCountry = $('#ConsignorCountry').val();
                var ConsignorCity = $('#ConsignorCity').val();
                var ConsigneeCountry = $('#ConsigneeCountry').val();
                var ConsigneeCity = $('#ConsigneeCity').val();
                var CourierStatus = $('#CourierStatus').val();
                var CourierStatusId = $('#CourierStatusId').val();
                var defaultdate = $('#AWBDate').val();


                var i = $('#tbl1 > tr').length;
