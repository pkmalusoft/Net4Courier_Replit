        $('#btnSave').click(function () {
            debugger;
            if ($('#EmployeeID').val() == '') {
                Swal.fire("Data Validation!", "Select Employee!", "error");
                return false;
            }
            else if ($('#EntryDate').val() == '') {
                Swal.fire("Data Validation!", "Select Entry Date!", "error");
                return false;
            }
            else if  ($('#ChangeCourierStatusId').val() == '') {
                Swal.fire("Data Validation!", "Select Status to Update!", "error");
                return false;
            }
            // $('#btnSave').attr('disabled', 'disabled');
            var obj = [];
            var maxrow = $('#tbl1 > tr').length;
            if (maxrow == 0) {
                Swal.fire("Data Validation!", "Add AWB No. to update Status!", "error");
                return;
            }
            for (i = 0; i < maxrow; i++) {
                debugger;
                var deleted = $('#hdndeleted_' + i).val();
                if (deleted == 'false') {
                    var InScanID = $('#hdnInScanID_' + i).val();
                    var ShipmentID = $('#hdnShipmentDetailID_' + i).val();
                    var AWBNo = $('#hdnAWBNo_' + i).val();

                    var item = {
                        InScanID: InScanID,
                        ShipmentID: ShipmentID,
                        AWBNo: AWBNo

                    }

                    obj.push(item);
                }

                if (maxrow == (i + 1)) {

                    $.ajax({
                        type: "POST",
                        url: "/AWBStatus/SaveStatus",
                        datatype: "Json",
                        data: {
                            EmployeeID: $('#EmployeeID').val(), EntryDate: $('#EntryDate').val(), CourierStatusId: $('#ChangeCourierStatusId').val(),Remarks :$('#Remarks').val(), Details: JSON.stringify(obj) },
                        success: function (response) {
                            //console.log(response);

                            debugger;
                            if (response.status == "OK") {
                                Swal.fire("Save Status!", response.message, "success");
                                $('#btnSave').removeAttr('disabled');

                                setTimeout(function () {
                                    window.location.href = "/AWBStatus/Create";

                                }, 300)
                            }
                            else {

                                Swal.fire("Save Status!", response.message, "error");
                                $('#btnSave').removeAttr('disabled');

                                return false;
                            }
                        },
                        error: function (err) {
                            console.log(err);
                        }
                    });
                }
            }
        });
    });
</script>

