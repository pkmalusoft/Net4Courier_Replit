                id = 0;
                $("#DestinationCountry").val("");
                $("#ConsigneeAddress").val("");
            }
            if (id > 0) {
                $.get("/ExportShipment/GetAgentBy_Id", { Id: id }, function (data) {
                    $("#DestinationCountry").val(data.CountryId);
                    $("#ConsigneeAddress").val(data.address);

                    // Addshipment(data.CountryId);

                });
            };
        });

