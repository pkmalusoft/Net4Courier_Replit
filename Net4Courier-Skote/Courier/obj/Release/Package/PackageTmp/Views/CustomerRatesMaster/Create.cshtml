                    //    + '<td>' + value.AddRate + '<input type="hidden" value=' + value.AddRate + ' name="CustRateDetails[' + i + '].AddRate"></td>' +
                    //    '<td><a href="javascript:void(0)" onclick="check(this)" value="Click"><i class="fa fa-pencil"></i></a>' +
                    //    '<a href="javascript:void(0)" onclick="deletetrans(this,' + i + ')"><i class="fa fa-times"></i></a></td>' +
                    //    '</tr> ');
                    //CustomerRateDetID

                    var html = '<tr><td><input type="hidden" name="CustRateDetails[' + i + '].CustomerRateDetID" value ="' + value.CustomerRateDetID + '" id = "hdnCustomerRateDetID_' + i + '" /><input class="hdndeleted" type="hidden" name="CustRateDetails[' + i + '].Deleted" value = "false" id = "hdndeleted_' + i + '" /><input type="text" class="form-control-sm text-right" id="txtWtFrom_' + i + '" value="' + value.AddWtFrom  + '" name="CustRateDetails[' + i + '].AddWtFrom" /></td>' +
                        '<td><input type="text" class="form-control-sm text-right" id="txtWtTo_' + i + '"  value="' + value.AddWtTo + '" name="CustRateDetails[' + i + '].AddWtTo"></td>' +
                        '<td><input type="text"  class="form-control-sm text-right" id="txtIncrWt_' + i + '" value="' + value.IncrWt + '" name="CustRateDetails[' + i + '].IncrWt"></td>' +
                        '<td><input type="text" class="form-control-sm text-right"  id="txtContractRate_' + i + '" value="' + x + '" name="CustRateDetails[' + i + '].ContractRate" onchange="contractdatecal(' + i + ')" ></td>' +
                        '<td><input type="text"  class="form-control-sm text-right crate" id="txtAddRate_' + i + '" value="' + value.AddRate  + '" name="CustRateDetails[' + i + '].AddRate"></td>' +
                        //'<td><a href="javascript:void(0)" onclick="check(this)" value="Click"><i class="fa fa-pencil"></i></a></td>' +
                        '<td><a class="text-danger" href="javascript:void(0)" onclick="deletetrans(this,' + i + ')"><i class="mdi mdi-delete font-size-18"></i></a></td>' +
                        +'</tr>';
                    $("#details").append(html);

                   // var i = $('#details > tr').length - 1;
                    var temp = parseFloat($('#txtWtTo_' + i).val()) + parseFloat("0.001");
                    // var temp = parseFloat($("#details > tr:last").find("td:eq(1)").text()) + parseFloat("0.001");
                    //$("#addwtfrom").val(parseFloat(temp).toFixed(3));
                    $('#addwtfrom').val(getmaxweight());
                    $('#incrwt').val(getmaxIncrweight());
                });


            }

        });
    }
