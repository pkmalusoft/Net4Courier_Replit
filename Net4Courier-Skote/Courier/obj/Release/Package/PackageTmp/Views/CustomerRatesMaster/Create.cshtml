@using (Html.BeginForm("Create", "CustomerRatesMaster", FormMethod.Post, new { @class = "needs-validation ", @novalidate = "novalidate", @id = "quickAWB" }))
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)
    <div id="validations" style="color: red; margin-left: 7px; display: none">* Please fill mandatory fields</div>

    <div class="card">
        <div class="card-body">
            <div class="row">
                <div class="col-md-12 text-md-end btn_top">
                    <div class="mt-2">
                        <input type="submit" value="Save" class="btn btn-success" id="btnsave" />
                        <a href='@Url.Action("Index", "CustomerRatesMaster", new { id = 0 })' class="btn btn-secondary">Cancel</a>
                    </div>
                </div>
            </div>
            <div class="row">

                <div class="col-md-3">
                    <div class="mb-2">
                        @Html.HiddenFor(model => model.CustomerRateID)
                        @Html.HiddenFor(model => model.CustomerRateTypeID)

                        <div class="flexTitle">
                            <label class="col-form-label required">Contract Name</label>
                            <div class="checkboxdesign no-padding  text-right" style="float:right">
                                <label class="col-form-label" style="color:#07a7e3!important;padding-left: 0!important" id="lblRateType">Standard</label>
                            </div>
                        </div>
                        @Html.DropDownListFor(model => model.ContractRateTypeID, new SelectList(@ViewBag.CustRate, "CustomerRateTypeID", "CustomerRateType1"), "Select", new { @class = "form-select" })
                        @Html.ValidationMessageFor(model => model.ContractRateTypeID)
                    </div>
                </div>


                <div class="col-md-3">
                    <div class="mb-2">
                        <label class="col-form-label required">Zone</label>
                        @*@Html.DropDownListFor(model => model.ZoneChartID, new SelectList(@ViewBag.Zones, "ZoneChartID", "ZoneChartID"), "Select", new { @class = "form-control", @value1 = Model.ZoneChartID })*@
                        @Html.TextBoxFor(model => model.ZoneChartName, new { @class = "form-select" })
                        @Html.ValidationMessageFor(model => model.ZoneChartID)
                        @Html.HiddenFor(model => model.ZoneChartID)

                    </div>
                </div>
                <div class="col-xs-12 col-sm-2">
                    <div class="mb-2">
                        <label class="required col-form-label">Movement Type</label>
                        @Html.HiddenFor(model => model.ZoneType, new { @class = "form-control", @readonly = "readonly" })
                        @Html.DropDownListFor(model => model.MovementID, new SelectList(@ViewBag.Movement, "MovementID", "MovementType"), "Select", new { @class = "form-select", @required = "true" })
                        @*@Html.HiddenFor(model => model.MovementID)*@

                    </div>
                </div>
                <div class="col-md-4">
                    <div class="mb-2">
                        <label class="col-form-label required">Countries/Cities</label>
                        @Html.TextBoxFor(model => model.CitiesCountries, new { @class = "form-control", @readonly = "readonly" })

                    </div>
                </div>
                <div class="col-md-3">
                    <div class="mb-2">
                        <label class="col-form-label required">Product Type</label>
                        @Html.DropDownListFor(model => model.ProductTypeID, new SelectList(@ViewBag.ProductType, "ProductTypeID", "ProductName"), "Select", new { @class = "form-select" })
                        @Html.ValidationMessageFor(model => model.ProductTypeID)
                    </div>
                </div>

                <div class="col-md-3">
                    <div class="mb-2">
                        <label class="col-form-label required">Forwarding Agent</label>
                        @Html.DropDownListFor(model => model.FAgentID, new SelectList(@ViewBag.ForwardingAgent, "SupplierID", "SupplierName"), "Select", new { @class = "form-select" })
                        @Html.ValidationMessageFor(model => model.FAgentID)

                    </div>
                </div>
                <div class="col-xs-12 col-sm-2">
                    <label class="col-form-label">Payment Mode</label>
                    @Html.DropDownListFor(model => model.PaymentModeID, new SelectList(@ViewBag.PaymentMode, "ID", "PaymentModeText"), "Select", new { @class = "form-select", @required = "true" })
                </div>

                <div class="col-xs-12 col-sm-2 hide" id="divMargin">
                    <label class="col-form-label">Margin Percentage</label>
                    @Html.TextBoxFor(model => model.BaseMargin, new { @class = "form-control textright" })
                </div>

                <div class="col-md-3" id="divWeight">
                    <div class="row">

                        <div class="col-md-6">
                            <label class="col-form-label required">Base Weight</label>
                            @Html.TextBoxFor(model => model.BaseWt, new { @class = "form-control" })
                            @Html.ValidationMessageFor(model => model.BaseWt)
                            @Html.HiddenFor(model => model.hfAddCustomerRate)
                        </div>

                        <div class="col-md-6">
                            <div class="flexTitle">
                                <label class="col-form-label required">B. Rate</label>
                                <div class="checkboxdesign no-padding  text-right" style="float:right">
                                    @Html.CheckBox("withtax", new { @name = "withtax", @id = "withtax" }) &nbsp
                                    <label class="col-form-label" style="color:#07a7e3!important;padding-left: 0!important">With Tax</label>
                                </div>
                            </div>
                            @Html.TextBoxFor(model => model.BaseRate, new { @class = "form-control" })
                            @Html.ValidationMessageFor(model => model.BaseRate)

                        </div>
                    </div>
                </div>


            </div>
        </div>
    </div>

}

