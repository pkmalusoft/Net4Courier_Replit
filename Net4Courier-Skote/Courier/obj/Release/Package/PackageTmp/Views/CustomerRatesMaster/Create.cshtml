@model Net4Courier.Models.CustRateVM
@{

    ViewBag.pTitle = "Rate Chart";
    ViewBag.pageTitle = "Setup";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script type="text/javascript">
    var flagedit = 0;
    var currentindex = 0;
    var nextindex = 0;
    var lastindex = 0;
    var nextcontractrate = 0;
    function getmaxweight() {
        debugger;

        var maxweight = 0;
        var maxrow = $('#details > tr').length;
        if (maxrow == 0) {
            maxweight = parseFloat($('#BaseWt').val()) + parseFloat('.001');
            return maxweight;
        }

        for (var i = maxrow; i >= 0; i--) {
            if ($('#hdndeleted_' + i).val() == 'false') {
                maxweight = parseFloat($('#txtWtTo_' + i).val()) + parseFloat('.001');
                return maxweight;
            }
        }

    }
    function getmaxIncrweight() {
        debugger;

        var maxweight = 0;
        var maxrow = $('#details > tr').length;
        if (maxrow == 0) {
            return 0.5;
        }
        for (var i = maxrow; i >=0; i--) {
            if ($('#hdndeleted_' + i).val() == 'false') {
                maxweight = parseFloat($('#txtIncrWt_' + i).val());
                return maxweight;
            }
        }

    }
    function getmaxbaserate() {
        var idtext = 'txtContractRate_';
        var maxweight = 0;
        var maxrow = $('#details > tr').length;
        if (maxrow == 0) {
            return $('#BaseRate').val();
        }
        for (var i = maxrow; i >= 0; i--) {
            if ($('#hdndeleted_' + i).val() == 'false') {
                maxweight = $('#txtContractRate_' + i).val();
                return maxweight;
            }
        }

    }

    function getmaxbaserate1(index) {
        var idtext = 'txtContractRate_';
        var maxweight = 0;
        var maxrow = $('#details > tr').length - 1;
        for (var i = index; i >= 0; i--) {
            if ($('#hdndeleted_' + i).val() == 'false') {
                maxweight = $('#txtContractRate_' + i ).val();
                return maxweight;
            }
        }

    }
    function contractdatecal(index) {
        debugger;
        if (index == 0 || index==-1) //|| $("#hfAddCustomerRate").val() == 0) {
        {
            if ($("#BaseRate").val() != "" && $("#txtContractRate_" + index).val() != "") {
                var AddittionalRate = parseFloat($("#txtContractRate_" + index).val()) - parseFloat($("#BaseRate").val());

                $("#txtAddRate_" + index).val(AddittionalRate.toFixed(2));
            }
        }
        else {

            var baserate = parseFloat(getmaxbaserate1(index-1));
            var decAdditionalRate;
            decAdditionalRate = parseFloat($("#txtContractRate_" + index).val()) - parseFloat(baserate);
            $("#txtAddRate_" + index).val(decAdditionalRate.toFixed(2));
            $("#txtContractRate_" + index).val(parseFloat($("#txtContractRate_" + index).val()).toFixed(2));
        }

    }
