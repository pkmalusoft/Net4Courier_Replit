@model IEnumerable<Net4Courier.Models.ImportShipment>

<thead>
    <tr>
        <th style="width:10%" >
            Manifest Number
        </th>
        <th style="width:13%">
            @Html.DisplayNameFor(model => model.FlightDate)
        </th>
        <th style="width:8%"> 
            Flight No.
        </th>
        <th style="width:10%">
            @Html.DisplayNameFor(model => model.MAWB)
        </th>
        <th style="width:5%">
            @Html.DisplayNameFor(model => model.CD)
        </th>
        <th style="width:5%">
            @Html.DisplayNameFor(model => model.Bags)
        </th>
        <th style="width:5%">
            @Html.DisplayNameFor(model => model.RunNo)
        </th>
        <th style="width:10%">
            @Html.DisplayNameFor(model => model.Type)
        </th>
        <th style="width:8%">
            Total AWB
        </th>    
        <th style="width:12%">
            Created Date
        </th>
        <th style="width:8%">Action</th>
    </tr>
</thead>
<tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ManifestNumber)
            </td>
            <td class="text-right">
                @Html.DisplayFor(modelItem => item.FlightDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FlightNo)
            </td>
            <td class="text-right">
                @Html.DisplayFor(modelItem => item.MAWB)
            </td>
            <td class="text-right">
                @Html.DisplayFor(modelItem => item.CD)
            </td>
            <td class="text-right">
                @Html.DisplayFor(modelItem => item.Bags)
            </td>
            <td class="text-right">
                @Html.DisplayFor(modelItem => item.RunNo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Type)
            </td>
            <td class="text-right">
                @Html.DisplayFor(modelItem => item.TotalAWB)
            </td>           
            <td class="text-right">
                @Html.DisplayFor(modelItem => item.CreatedDate)
            </td>
            <td>
                @*<a target="_blank" href="AgentShipmentReport?id=@item.ID"><i class="fa fa-print"></i></a>*@
                <a href="Details?id=@item.ID"><i class="fa fa-eye"></i></a>
                <a href="Create?id=@item.ID"><i class="fa fa-pencil"></i></a>
                <a href="#" onclick="deleteImport(@item.ID, 'AgentShipment')"><i class="fa fa-times"></i></a>                
            </td>
        </tr>
    }
</tbody>
