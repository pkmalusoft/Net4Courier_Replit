
@{
    ViewBag.Title = "Page Access Denied";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using (Html.BeginForm())
{
    @Html.ValidationSummary(true)

    <div class="container">
        <div class="row">
            <div class="col-md-4 col-md-offset-4">
                <br />
                <br />
                <div class="form-group">

                    <div class="alert alert-danger alert-dismissable" style="background-color:red"  role="alert" id="divErrorMessage">
                   
                        <h4><i class="mdi mdi-close-box"></i>You don't have access to this Page!</h4>
                        <div id="divFinalError"></div>
                    </div>
                </div>
                <br />
                @Html.ActionLink("CLICK HERE TO HOME", "Home", "EmployeeMaster")
            </div>
        </div>
    </div>
}


