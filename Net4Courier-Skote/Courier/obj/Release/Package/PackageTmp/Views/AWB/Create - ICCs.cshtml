                                GetcourierChargenew();
                                status = "ok";
                            }

                            if ((i + 1) == data.length) {
                                if (status == '') {
                                    $('#CustomerRateType').val(data[0].CustomerRateType1);
                                    $('#CustomerRateTypeID').val(data[0].CustomerRateTypeID);
                                    GetcourierChargenew();
                                    status = "ok";
                                }
                            }
                        }
                    }
                    else {
                        $('#CustomerRateTypeID').val(0);
                        $('#CustomerRateType').val("");
                        $('#CourierCharge').val(0);
                    }
                }
            })
        }
