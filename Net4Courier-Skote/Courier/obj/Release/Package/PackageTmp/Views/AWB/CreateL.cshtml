        $.ajax({
            type: "Get",
            url: "/AWB/GetStaffNotes",
            datatype: "Json",
            data: { InScanId: $('#InScanID').val() },
                success: function (response) {
                    if (response.status == "ok") {
                         $("#notesdetailsbody").html('');
                        $.each(response.data, function (index, item) {
                            debugger;
